{
  "name": "prime-nestjs",
  "version": "1.0.0",
  "description": "The ultimate collection of production-ready and flexible NestJS Boilerplate.",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node --transpile-only -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/config/typeorm.config.ts",
    "schema:sync": "npm run typeorm -- schema:sync",
    "schema:log": "npm run typeorm -- schema:log",
    "add:migration": "npm run typeorm -- migration:generate ./src/database/migrations/$npm_config_name",
    "apply:migration": "npm run typeorm migration:run --",
    "revert:migration": "npm run typeorm migration:revert --",
    "drop:database": "npm run typeorm schema:drop --",
    "migration:show": "npm run typeorm -- migration:show",
    "seed": "ts-node -r tsconfig-paths/register ./src/database/seed/seed.ts",
    "seed-test": "ts-node -r tsconfig-paths/register ./src/database/seed/seed.data.ts",
    "routes": "ts-node -r tsconfig-paths/register ./src/config/routes.ts",
    "generate:grpc": "npx grpc_tools_node_protoc --ts_out=src/shared/grpc/generate --grpc_out=src/shared/grpc/generate/grpc_js -I src/shared/grpc src/shared/grpc/$npm_config_name"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.1",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/bullmq": "^10.1.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.9",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.10",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.1",
    "@slack/events-api": "^3.0.1",
    "@slack/web-api": "^7.0.2",
    "@types/aws-sdk": "^2.7.0",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^10.0.0",
    "aws-crt": "^1.23.0",
    "aws-sdk": "^2.1653.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "file-type": "^16.5.3",
    "firebase-admin": "^12.7.0",
    "fuzzyset.js": "^1.0.7",
    "google-protobuf": "^3.21.2",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "htmldiff-js": "^1.0.5",
    "mqtt": "^5.10.2",
    "nestjs-i18n": "^10.4.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.11",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "swagger-ui-express": "^5.0.0",
    "ts-proto": "^2.0.3",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.19",
    "typeorm-polymorphic": "^1.0.0",
    "uuid": "^10.0.0",
    "vnpay": "^1.6.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-transport": "^4.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.17",
    "@types/fuzzyset.js": "^0.0.5",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.6.1",
    "@types/prettier": "^2.3.2",
    "@types/rimraf": "^3.0.2",
    "@types/supertest": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "grpc-tools": "^1.12.4",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "protoc-gen-ts": "^0.8.7",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
